#include "main.h"

 /**
  * *_strcat - concatenation of 2 strings.
  * @dest: destination
  * @src: source
  * Return: the pointer to dest
  */

char *_strcat(char *dest, char *src)
{
	char *s = dest;

	while (*dest != '\0')
	{
		dest++;
	}

	while (*src != '\0')
	{
		*dest = *src;
		dest++;
		src++;
	}
